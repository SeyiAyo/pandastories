{
  "buildCommand": "python vercel_build.py",
  "builds": [
    {
      "src": "pandastories/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": ["staticfiles/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pandastories/wsgi.py"
    }
  ]
}
